{
	"kind": "zodObject",
	"fields": {
		"token": {
			"type": "string",
			"required": true,
			"unique": false,
			"private": false,
			"minLength": 1
		},
		"password": {
			"type": "string",
			"required": true,
			"unique": false,
			"private": false,
			"minLength": 8,
			"maxLength": 100,
			"pattern": "^(?=.*[a-z])(?=.*[A-Z])(?=.*\\d)(?=.*[!@#$%^&*()_+\\-=\\[\\]{};':\"\\\\|,.<>/?]).{8,}$"
		},
		"confirmPassword": {
			"type": "string",
			"required": true,
			"unique": false,
			"private": false
		}
	}
}
