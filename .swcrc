{
	"$schema": "https://swc.rs/schema.json",
	"env": { "targets": "defaults" },
	"jsc": {
		"parser": {
			"syntax": "typescript",
			"tsx": true,
			"decorators": false
		},
		"transform": {
			"react": {
				"runtime": "automatic",
				"refresh": true
			}
		}
	},
	"module": { "type": "es6" },
	"sourceMaps": true
}
